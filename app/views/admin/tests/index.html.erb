<div>
  <%= link_to t('item.link.new_test'), new_admin_test_path, :class => 'btn btn-secondary m-2'%>
</div>

<div class="row justify-content-center mt-4">
  <div class="col-12 col-md-8">
    <table class="table caption-top table-hover align-middle mt-3">
      <caption><%= t('.header') %> </caption>
      <thead class="table-dark">
      <th scope="col"><%= t('.title') %></th>
      <th scope="col" class="text-center"><%= t('.level') %></th>
      <th scope="col" class="text-center"><%= t('.category') %></th>
      <th scope="col" class="text-center"><%= t('.author') %></th>
      <th scope="col"><%= t('.q_count') %></th>
      <th scope="col" colspan="3"><%= t('.actions')%></th>
      </thead>
      <%= render @tests %>
    </table>
  </div>
</div>

