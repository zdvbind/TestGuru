<p>
  <strong>Title:</strong>
  <%=@test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%=@test.level %>
</p>

<p>
  <strong>Author:</strong>
  <%= @test.author.email %>
</p>

<p>
  <strong>Category:</strong>
  <%= @test.category.title %>
</p>
<h3>Test's questions</h3>
<table>
  <thead>
  <tr>
    <th>Body</th>
    <th colspan="3">Actions</th>
  </tr>
  </thead>
  <tbody>
    <%= render @test.questions %>
    <%#= render partial: 'questions/question', collection: @test.questions %>
  </tbody>
</table>
<%= link_to 'Edit', edit_admin_test_path(@test) %> |
<%= link_to 'Back', admin_tests_path %> |
<%= link_to 'New question', new_admin_test_question_path(@test) %>